#include<stdio.h>
int main (){
   //   输错三次密码就退出
	int i=1,password;
	
	while(i<=3)
	{
		printf("请输入密码");
		scanf("%d",&password);
		if(password==123456)
		 {
			printf("密码输入正确");
			break;
		 }
		else
		{
			printf("密码输入错误，当前是第%d次输入\n",i);
			
		
			 if(i==3)
			 {
				printf("码输入超过3次，系统退出");
			 }
			 i++;
		 }
	
	}
	 
}
	
	
	







